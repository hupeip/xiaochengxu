/* pages/share/share.wxss */
.img{
   display: block;
}
.banner{
  width: 750rpx; 
  height: 108rpx;
}
.wrapper {
  width: 100%;
  background: url(data:image/jpeg;base64,) repeat-y center;
  background-size: 750rpx 68rpx;
}
.title {
  font-size: 32rpx;
  color: #003333;
  text-align: center;
  padding: 40rpx 0 35rpx;
  position: relative;
}
.flower {  
  width: 77rpx;
  height: 77rpx; 
}
.flower-1 {
  position: absolute;
  right: 34rpx;
  top: 20rpx;
}
.name {
  color: #e20d6a;
}
.info-block {
  text-align: center;
  margin-top: 90rpx;
  position: relative;
}
.info-avatar {
  display: inline-block;
  text-align: center;
  width: 250rpx;
  height: 250rpx;
  border: 2rpx solid #faabbe;
  border-radius: 50%;
  
}
.avatar {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 4rpx solid #d0104c;
  margin: 45rpx 0 100rpx;
}
.mark {
  position: absolute;
  height: 60rpx;
  color: #fff;
  line-height: 60rpx;
  font-size: 32rpx;
  border-radius: 10rpx;
  padding: 0 22rpx;
}
.mark1 {
  background-color: #c73e3a;
  top: -87rpx;
  right: 352rpx;  
}
.mark2 {
  background-color: #764a6b;
  top: -50rpx;
  left: 437rpx;
}
.mark3 {
  background-color: #9b90c2;
  top: 10rpx;
  right: 450rpx;
}
.mark4 {
  background-color: #f19483;
  top: 50rpx;
  left: 480rpx;
}
.mark5 {
  background-color: #5dac81;
  top: 143rpx;
  right: 460rpx;
}
.mark6 {
  background-color: #00896c;
  top: 163rpx;
  left: 470rpx;
}
.mark7 {
  width: 100%;
  bottom: -23rpx;
  text-align: center;
}
.item-7 {
  display: inline-block;
  background-color: #d0104c;
  line-height: 60rpx;
  font-size: 32rpx;
  border-radius: 10rpx;
  padding: 0 22rpx;
}
.text {
  color: #e20d6a;
  font-size: 36rpx;
  font-weight: bold;
  text-align: right;
  margin: 90rpx 90rpx 0 0;
}
.tips-box {
  padding: 0 25rpx;
}
.tips-block {
  box-sizing: border-box;
  padding: 20rpx 0 20rpx 187rpx;
  width: 100%;
  background: rgba(232, 122, 134, .3);
  margin-top: 10rpx;
  border-radius: 20rpx;
  position: relative;
}
.tips {
  font-size: 28rpx;
  color: #d0104c;
  display: inline-block;
}
.big-avatar {
  position: absolute;
  bottom: 0;
  left: 0;
}
.big-caier {
  width: 192rpx;
  height: 206rpx; 
}
.bottom-block {
