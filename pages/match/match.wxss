/* pages/match/match.wxss */

.img{
   display: block;
}
.banner{
  width: 750rpx; 
  height: 124rpx;
}
.wrapper {
  width: 100%;
  background: url(data:image/jpeg;base64,) repeat-y center;
  background-size: 750rpx 68rpx;
}
.info-block {
  position: relative;
}
.info-box {
  display: flex;
  padding-left: 40rpx;
  position: relative;
}
.info-img{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  border: 4rpx solid #d0104c;
  margin-top: 30rpx;
}
.info-center {
  position: relative;
  margin: 0 40rpx;
  width: 254rpx;
  height: 254rpx;
}
.out-circle {
  position: absolute;
  width: 254rpx;
  height: 254rpx;
  border: 3rpx solid #fbdfe6;
  border-radius: 50%;
  text-align: center;  
  animation: out 2s infinite linear;  
}
.inner-circle {
  position: absolute;
  width: 216rpx;
  height: 216rpx;
  border: 2rpx solid #ffc1d4;
  border-radius: 50%;
  display: inline-block;
  left: 18rpx;
  top: 18rpx;
  animation: inner 2s infinite linear;
} 
.rotate {
  position: absolute;
  left: 18rpx;
  top: 18rpx;
}
.rotate-img {
  width: 218rpx;
  height: 218rpx;
  display: inline-block;
  animation: rotate 2s infinite linear;
}
.match-box {
  position: absolute;
  width: 100%;
  text-align: center;
  top: 50rpx;
}
@keyframes out {
  0% {
    width: 254rpx;
    height: 254rpx;
    left: 0;
    top: 0;
  }
  50% {
    width: 216rpx;
    height: 216rpx;
    left: 18rpx;
    top: 18rpx;
  }
  100% {
    width: 254rpx;
    height: 254rpx;
    left: 0;
    top: 0;
  }
}
@keyframes inner {
  0% {
    width: 216rpx;
    height: 216rpx;
    left: 18rpx;
    top: 18rpx;
  }
  50% {
    width: 254rpx;
    height: 254rpx;
    left: 0;
    top: 0;
  }
  100% {
    width: 216rpx;
    height: 216rpx;
    left: 18rpx;
    top: 18rpx;
  }
}
@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
